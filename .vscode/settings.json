{
  "editor.fontFamily": "'IntelOne Mono', Consolas, 'Courier New', monospace",
  "eslint.workingDirectories": ["./src/packages/preset", "./src/docs"],
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "wordWrapColumn",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLensFontFamily": "'IntelOne Mono'",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.dropIntoEditor.enabled": false,
  "editor.dropIntoEditor.showDropSelector": "never",
  "editor.emptySelectionClipboard": false,
  "editor.experimentalWhitespaceRendering": "off",
  "editor.folding": true,
  "editor.foldingImportsByDefault": false,
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.guides.bracketPairs": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.insertSpaces": true,
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 3,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.loop": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "diffEditor.codeLens": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.suggest.showStatusBar": true,
  "files.enableTrash": false,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.list.smoothScrolling": true,
  "workbench.quickOpen.preserveInput": true,
  "workbench.startupEditor": "readme",
  "workbench.colorTheme": "Dracula Refined",
  "workbench.preferredDarkColorTheme": "Dracula Refined",
  "workbench.iconTheme": "material-icon-theme",
  "breadcrumbs.enabled": true,
  "workbench.settings.useSplitJSON": true,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.expand": true,
  "scm.alwaysShowActions": false,
  "scm.alwaysShowRepositories": true,
  "scm.autoReveal": true,
  "scm.defaultViewMode": "tree",
  "scm.providerCountBadge": "hidden",
  "eslint.packageManager": "pnpm",
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 80,
  "html.format.wrapAttributes": "auto",
  "html.format.wrapAttributesIndentSize": 2,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.hidesExplorerArrows": false,
  "npm-intellisense.importQuotes": "\"",
  "npm-intellisense.showBuildInLibs": true,
  "npm-intellisense.scanDevDependencies": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "prettier.requireConfig": true,
  "prettier.singleAttributePerLine": true,
  "tailwindCSS.classAttributes": ["class", "className"],
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.lint.cssConflict": "error",
  "tailwindCSS.lint.recommendedVariantOrder": "error",
  "javascript.format.insertSpaceAfterConstructor": false,
  "javascript.preferGoToSourceDefinition": true,
  "js/ts.implicitProjectConfig.target": "ESNext"
}
