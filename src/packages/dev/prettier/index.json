{
  "singleQuote": false,
  "semi": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSameLine": false,
  "printWidth": 80,
  "quoteProps": "as-needed",
  "singleAttributePerLine": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "requirePragma": false
}
